{
    "name": "search-react-express",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node app.js",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "server": "nodemon app.js",
        "client": "npm start --prefix client",
        "dev": "concurrently --kill-others \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "concurrently": "^7.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "mongoose": "^6.2.7"
    },
    "devDependencies": {
        "nodemon": "^2.0.15"
    }
}
